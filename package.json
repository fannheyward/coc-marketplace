    "url": "https://github.com/fannheyward/coc-marketplace.git"
  },
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.73"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "coc.nvim": "^0.0.79",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {},
  "prettier": {
    "printWidth": 160,
    "singleQuote": true
  },
  "contributes": {
